<div class="show-container">
  <p id="notice"><%#= notice %></p>
  <p>
    <%= @blog.date_created %>
  </p>

  <h1><%= @blog.title %></h1>

  <p>
    <%= @blog.content.html_safe %>
  </p>

  <div class="links">
    <%= link_to 'Edit', edit_blog_path(@blog) %> |
    <%= link_to 'Back', blogs_path %>
  </div>
</div>
